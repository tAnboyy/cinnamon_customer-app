{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*"
    ],
    "headers": [
      {
        "source": "**/*.@(ttf|otf|woff|woff2|eot)",
        "headers": [
          {
            "key": "Content-Type",
            "value": "font/ttf"
          },
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      },
      {
        "source": "**/*.woff",
        "headers": [
          {
            "key": "Content-Type",
            "value": "font/woff"
          }
        ]
      },
      {
        "source": "**/*.woff2",
        "headers": [
          {
            "key": "Content-Type",
            "value": "font/woff2"
          }
        ]
      }
    ],
    "rewrites": [
      {
        "source": "!/@(assets|_expo)/**",
        "destination": "/index.html"
      }
    ]
  }
}
