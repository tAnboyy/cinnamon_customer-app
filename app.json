{
  "expo": {
    "name": "Cinnamon",
    "scheme": "cinnamon",
    "slug": "cinnamon",
    "plugins": [
      "@react-native-google-signin/google-signin",
      [
        "@stripe/stripe-react-native",
        {
          "enableGooglePay": true,
          "publishableKey": "pk_test_51SgYBZSFi2GeyF3cBzykSmU9mNvggPy72eL5dq5xgAJ6rig7wSNF6oOgJX2V1uz9nBLpgs8uDS8CIPW4NCU26767006xwB1d1F"
        }
      ],
      "@react-native-community/datetimepicker"
    ],
    "android": {
      "package": "com.tandevv.cinnamon_customer",
      "googleServicesFile": "./google-services.json",
      "statusBar": {
        "backgroundColor": "#ffffff",
        "barStyle": "dark-content",
        "translucent": false
      }
    },
    "ios": {
      "bundleIdentifier": "com.tandevv.cinnamon-customer",
      "googleServicesFile": "./GoogleService-Info.plist",
      "statusBar": {
        "barStyle": "dark"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "output": "single",
      "bundler": "metro"
    },
    "extra": {
      "expoPublicGoogleWebClientId": "${EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID}",
      "expoPublicGoogleIosClientId": "${EXPO_PUBLIC_GOOGLE_IOS_CLIENT_ID}",
      "eas": {
        "projectId": "7f0e199c-2420-4dfc-bb87-9ef6d5121386"
      }
    },
    "owner": "cinnamon-org"
  }
}
